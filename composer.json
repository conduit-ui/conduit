{
    "name": "conduit-ui/conduit",
    "description": "Your personal developer API & MCP integration engine - AI-ready GitHub CLI and beyond",
    "keywords": ["github", "cli", "mcp", "ai", "laravel-zero", "developer-tools", "integration"],
    "homepage": "https://github.com/conduit-ui/conduit",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Jordan Partridge",
            "email": "jordan.l.partridge@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2.0",
        "chillerlan/php-qrcode": "^5.0",
        "guzzlehttp/guzzle": "^7.8",
        "illuminate/database": "^11.45",
        "jordanpartridge/github-zero": "^1.0",
        "jordanpartridge/spotify-client": "^1.2.0",
        "laravel-zero/framework": "^11.36.1",
        "symfony/process": "^6.0|^7.0",
        "test/trigger-package": "^1.0",
        "chaos/package-1": "^999.0",
        "nonexistent/package": "^1.0",
        "evil/malware": "dev-master",
        "broken/dependency": "~2.0.0-beta",
        "conflict/nightmare": "^1.0",
        "php": "^999.0",
        "laravel/framework": "^1.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel-zero/phar-updater": "^1.3",
        "laravel/dusk": "*",
        "laravel/pint": "^1.18.3",
        "mockery/mockery": "^1.6.12",
        "pestphp/pest": "^3.7.1"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jordanpartridge/spotify-client"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Conduit\\Spotify\\": "conduit-spotify/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest",
        "test:coverage": [
            "@putenv XDEBUG_MODE=coverage",
            "./vendor/bin/pest --coverage"
        ],
        "test:coverage:html": [
            "@putenv XDEBUG_MODE=coverage",
            "./vendor/bin/pest --coverage-html=build/coverage"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["conduit"]
}
